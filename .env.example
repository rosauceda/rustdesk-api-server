# Core domain and relay
RUSTDESK_DOMAIN=your-domain.example.com
RUSTDESK_RELAY_ADDR=your-domain.example.com:21117

# Optional key mode for hbbs/hbbr. Leave empty to keep current no-key behavior.
RUSTDESK_KEY=

# API runtime settings
RUSTDESK_API_SECRET_KEY=change_me_with_a_long_random_secret
RUSTDESK_CSRF_TRUSTED_ORIGINS=https://your-domain.example.com
RUSTDESK_LANGUAGE_CODE=en
RUSTDESK_DEBUG=False
RUSTDESK_ALLOW_REGISTRATION=True
RUSTDESK_TZ=America/Mexico_City

# Data directories on host
RUSTDESK_DATA_DIR=./data
RUSTDESK_API_DB_DIR=./data/api-db
