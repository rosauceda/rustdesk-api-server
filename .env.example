# Core domain and relay
RUSTDESK_RELAY_ADDR=your-domain.example.com:21117

# Optional key mode for hbbs/hbbr. Leave empty to keep current no-key behavior.
RUSTDESK_KEY=
ALWAYS_USE_RELAY=N

# API runtime settings
SECRET_KEY=change_me_with_a_long_random_secret
ID_SERVER=your-domain.example.com
CSRF_TRUSTED_ORIGINS=https://your-domain.example.com
LANGUAGE_CODE=en
DEBUG=False
ALLOW_REGISTRATION=True
TZ=America/Mexico_City

# Data directories on host
RUSTDESK_DATA_DIR=./data
RUSTDESK_API_DB_DIR=./data/api-db
